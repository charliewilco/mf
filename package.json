{
  "name": "mf-cli",
  "version": "3.0.0",
  "description": "A nicer touch",
  "type": "module",
  "preferGlobal": true,
  "files": [
    "./cli.js",
    "./index.js"
  ],
  "bin": {
    "mf": "./cli.js"
  },
  "scripts": {
    "lint": "prettier ./src/*.ts -l",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --verbose",
    "_bin": "node ./cli.js"
  },
  "keywords": [
    "touch",
    "cli",
    "utility"
  ],
  "author": "Charlie ⚡️",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.1",
    "has-emoji": "^2.0.0",
    "make-dir": "^3.1.0",
    "picocolors": "^1.0.0",
    "tiny-esm-test-runner": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "del": "^6.0.0",
    "execa": "^6.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "transform": {}
  },
  "prettier": {
    "printWidth": 95,
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "repository": "charliewilco/mf",
  "bugs": {
    "url": "https://github.com/charliewilco/mf/issues"
  },
  "homepage": "https://github.com/charliewilco/mf#readme"
